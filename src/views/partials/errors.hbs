{{#if error}}
<div class="alert alert-warning d-flex justify-content-between alert-danger fade show" role="alert">
  {{error}}
  <button type="button" class="btn-close " data-bs-dismiss="alert" aria-label="Close"></button>
</div>
{{/if}}
{{#each error}}
  <div class="alert alert-warning d-flex justify-content-between alert-danger fade show" role="alert">
  {{text}}
  <button type="button" class="btn-close " data-bs-dismiss="alert" aria-label="Close"></button>
</div>
{{/each}}